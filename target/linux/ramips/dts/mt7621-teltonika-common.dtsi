#include <dt-bindings/input/input.h>
#include "mt7621.dtsi"
#include "platform_name.dtsi"

&pcie {
    status = "okay";
};

&pcie0 {
    wifi@0,0 {
        compatible = "mediatek,mt76";
        reg = <0x0000 0 0 0 0>;
        mediatek,mtd-eeprom = <&factory 0x0>;
    };
};

&gmac0 {
	mtd-mac-address = <&config 0x0>;
};

&gmac1 {
        status = "okay";
	label = "wan";
	phy-handle = <&ephy4>;
	mtd-mac-address = <&config 0x0>;
	mtd-mac-address-increment = <1>;
};

&mdio {
	ephy4: ephy@4 {
		reg = <4>;
	};

	ephy7: ephy@7 {
		reg = <7>;
	};
};

&crypto {
	status = "okay";
};
